#!/usr/bin/env perl

use lib 'lib';
use v5.10;
use strict;
use warnings;
use Data::Dumper;
use Lifter;

my $map;
if(-f $ARGV[0]) {
  $map = Lifter::load_map($ARGV[0]);
} else {
  # Contest-mode
  $map = Lifter::load_map(\*STDIN);
}

Lifter::print_map $map;

$map = Lifter::map_update($map);

Lifter::print_map $map;


